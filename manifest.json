{
  "manifest_version": 2,
  "name": "remove-pagination",
  "short_name": "rmvpage",
  "version": "0.1.0",
  "display": "browser",
  "author" : "Alfian Syarif", 
  "scope": "/",
  "start_url": "/",
  "icons": { 
    "72": "icons/icon-72.png",
    "96": "icons/icon-96.png",
    "128": "icons/icon-128.png" 
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
    ]
}